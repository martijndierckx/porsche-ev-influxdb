{
    "configurations": [
    {
      "name": "TS Debug file",
      "type": "node",
      "request": "launch",
      "args": [
        "${relativeFile}"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "envFile": "${workspaceFolder}/.env"
    }
    ]
}